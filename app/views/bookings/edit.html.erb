<div class="container">
  <div class="row">
    <div class="col-sm-4 col-sm-offset-4">
<div id="smallbanner">
<br>
<br>
<br>
<br>
  <h1 class="fontsmallbanner"> Edit your booking </h1>
<br>
<br>
      <%= form_for([@venue, @booking]) do |f| %>

      <div class="form-group">
        <%= f.label :initial_date %>
        <%= f.text_field :initial_date, class: 'form-control' %>
      </div>

      <div class="form-group">
        <%= f.label :final_date %>
        <%= f.text_field :final_date, class: 'form-control' %>
      </div>

      <%= f.submit class: 'btn btn-primary' %>
      <% end %>
    </div>
  </div>
</div>
